{
  "name": "4showstack",
  "version": "1.0.0",
  "description": "API Gateway Websocket Demo",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/paipe/4show/v2/4show-stack"
  },
  "scripts": {
    "prepare": "husky install",
    "build": "npm run build:web",
    "build:web": "ts-node esbuild.ts build",
    "clean": "rimraf cdk.out coverage web/js",
    "deploy": "npm run clean && cdk deploy --require-approval never --outputs-file config.json",
    "destroy": "cdk destroy --force",
    "diff": "cdk diff",
    "lint": "eslint . --ext=.js,.ts",
    "pretest": "npm run lint",
    "start": "npm run clean && ts-node esbuild.ts",
    "synth": "cdk synth --no-staging > template.yaml",
    "test": "jest --coverage --silent",
    "test:default": "node --expose-gc node_modules/.bin/jest --runInBand --logHeapUsage --detectOpenHandles --no-cache",
    "test:heap": "npm run test:default --silent",
    "test:heap:nosilent": "npm run test:default --no-cache",
    "test:heap:cov": "npm run test:default --silent --coverage",
    "test:fns": "npm run test:default -- ./tests/lambda-functions --silent",
    "test:fns:coverage": "npm run test:default -- ./tests/lambda-functions --silent --coverage",
    "test:cdk": "npm run test:default -- ./tests/cdk --silent"
  },
  "keywords": [
    "API",
    "Gateway",
    "Websocket",
    "CDK",
    "esbuild",
    "React"
  ],
  "author": "Jonathan Baraldi && Devs 4show",
  "license": "GNU",
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/aws-lambda": "^8.10.87",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^27.0.3",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^14.18.12",
    "@types/react-dom": "^17.0.11",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "aws-cdk": "^2.20.0",
    "dotenv": "^16.0.0",
    "esbuild": "^0.14.2",
    "esbuild-plugin-sass": "^0.6.0",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "fs-extra": "^10.0.0",
    "husky": "^8.0.1",
    "jest": "^27.4.4",
    "prettier": "^2.5.1",
    "react-test-renderer": "^17.0.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.3"
  },
  "dependencies": {
    "@aws-cdk/aws-apigatewayv2-alpha": "^2.1.0-alpha.0",
    "@aws-cdk/aws-apigatewayv2-integrations-alpha": "^2.1.0-alpha.0",
    "@aws-sdk/client-rds-data": "^3.67.0",
    "aws-cdk-lib": "^2.1.0",
    "aws-sdk": "^2.1087.0",
    "constructs": "^10.0.12",
    "dynamodb-toolbox": "^0.3.5",
    "generate-password": "^1.7.0",
    "jsonwebtoken": "^8.5.1",
    "ksuid": "^3.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-draggable": "^4.4.4",
    "react-use-websocket": "^2.9.1"
  }
}
